# Job Search API Configuration (example)
# Copy this file to config/job_search_config.env and customize values.

# Search terms (space separated, wrapped in quotes)
SEARCH_TERMS="\"data\" \"analytics\" \"python\" \"sql\" \"machine learning\""

# Target location context
SEARCH_WHERE="Berlin"

# Search radius in km
SEARCH_RADIUS_KM=40

# Only jobs published within N days
# Allowed values: 0 (today), 1 (yesterday), 7 (1 week), 14 (2 weeks), 28 (4 weeks).
SEARCH_DAYS=7
